{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "max-len": ["warn", 120],
    "indent": ["warn", 2],
    "no-eval": "warn",
    "quotes": ["warn", "single"],
    "jsx-quotes": ["warn", "prefer-double"],
    "no-unused-vars": "warn",
    "no-console": "off"
  },
  "extends": "eslint:recommended"
}
