{
  "name": "renku-ui",
  "version": "0.7.1",
  "private": true,
  "dependencies": {
    "@babel/helper-regex": "^7.4.4",
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-regular": "^5.0.10",
    "@fortawesome/fontawesome-free-solid": "^5.0.10",
    "@fortawesome/react-fontawesome": "0.0.18",
    "@nteract/notebook-render": "^5.0.4-alpha.0",
    "ajv": "^6.10.0",
    "apollo-boost": "^0.4.3",
    "bootstrap": "^4.0.0",
    "d3": "^5.9.2",
    "dagre-d3": "^0.6.1",
    "dompurify": "^2.0.0",
    "filesize": "^3.6.1",
    "graphql": "^14.4.2",
    "http-proxy": "^1.16.2",
    "human-time": "0.0.1",
    "immutability-helper": "^2.6.6",
    "immutable": "^4.0.0-rc.12",
    "jquery": "^3.4.1",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.15.0",
    "query-string": "^5.1.1",
    "react": "^16.8.6",
    "react-autosuggest": "^9.4.0",
    "react-clipboard.js": "^2.0.16",
    "react-collapse": "^4.0.3",
    "react-dom": "^16.8.6",
    "react-js-pagination": "^3.0.2",
    "react-media": "^1.9.2",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^8.0.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "showdown": "^1.9.0",
    "styled-jsx": "^2.2.6",
    "uuid": "^3.2.1",
    "yaml-js": "^0.2.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles --watch --recursive",
    "start-js": "REACT_APP_UI_DEV_MODE=$UI_DEV_MODE REACT_APP_RENKU_UI_URL=$RENKU_UI_URL react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && REACT_APP_UI_DEV_MODE=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "node_modules/eslint/bin/eslint.js src/",
    "lint-fix": "node_modules/eslint/bin/eslint.js src/ --fix",
    "node": "node_modules/.bin/babel-node"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "eslint-plugin-react": "^7.13.0",
    "fetch-mock": "^6.0.0-beta.7",
    "history": "^4.9.0",
    "react-scripts": "^3.0.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
