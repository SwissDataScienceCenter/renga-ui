{
  "name": "renga-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.0.0",
    "font-awesome": "^4.7.0",
    "human-time": "0.0.1",
    "jquery": "^3.2.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "popper.js": "^1.12.9",
    "qs": "^6.5.1",
    "react": "^16.2.0",
    "react-collapse": "^4.0.3",
    "react-dom": "^16.2.0",
    "react-fontawesome": "^1.6.1",
    "react-jupyter": "^0.1.0",
    "react-markdown": "^3.1.4",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "reactstrap": "^5.0.0-alpha.4",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "universal-cookie": "^2.1.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles --watch --recursive",
    "start-js": "REACT_APP_UI_DEV_MODE=$UI_DEV_MODE REACT_APP_GITLAB_URL=$GITLAB_URL REACT_APP_RENGA_UI_URL=$RENGA_UI_URL REACT_APP_KEYCLOAK_URL=$KEYCLOAK_URL react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && REACT_APP_UI_DEV_MODE=false REACT_APP_GITLAB_URL=$GITLAB_URL REACT_APP_RENGA_UI_URL=$RENGA_UI_URL REACT_APP_KEYCLOAK_URL=$KEYCLOAK_URL react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "node_modules/eslint/bin/eslint.js src/",
    "lint-fix": "node_modules/eslint/bin/eslint.js src/ --fix",
    "node": "node_modules/.bin/babel-node"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.16.0",
    "eslint-plugin-react": "^7.6.1",
    "fetch-mock": "^6.0.0-beta.7",
    "keycloak-js": "^3.4.3"
  }
}
