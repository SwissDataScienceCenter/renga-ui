{
  "name": "renku-ui",
  "version": "0.10.2",
  "private": true,
  "dependencies": {
    "@babel/helper-regex": "^7.8.3",
    "@ckeditor/ckeditor5-react": "^2.1.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@nteract/notebook-render": "^5.0.4-alpha.0",
    "@renku/ckeditor5-build-renku": "0.0.1",
    "@sentry/browser": "^5.12.1",
    "ajv": "^6.10.2",
    "apollo-boost": "^0.4.4",
    "bootstrap": "^4.0.0",
    "d3": "^5.12.0",
    "dagre-d3": "^0.6.1",
    "dompurify": "^2.0.7",
    "filesize": "^6.0.0",
    "graphql": "^14.5.8",
    "http-proxy": "^1.18.0",
    "human-time": "^0.0.2",
    "immutability-helper": "^3.0.1",
    "immutable": "^4.0.0-rc.12",
    "jquery": "^3.4.1",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.0",
    "query-string": "^6.8.3",
    "react": "^16.11.0",
    "react-autosuggest": "^10.0.0",
    "react-clipboard.js": "^2.0.16",
    "react-collapse": "^5.0.0",
    "react-dom": "^16.11.0",
    "react-js-pagination": "^3.0.2",
    "react-media": "^1.10.0",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^5.1.2",
    "reactstrap": "^8.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.2.0",
    "sass": "^1.23.7",
    "showdown": "^1.9.1",
    "styled-jsx": "^3.2.3",
    "uuid": "^3.3.3",
    "yaml-js": "^0.2.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles --watch --recursive",
    "start-js": "REACT_APP_UI_DEV_MODE=$UI_DEV_MODE REACT_APP_RENKU_UI_URL=$RENKU_UI_URL react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && REACT_APP_UI_DEV_MODE=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "node_modules/eslint/bin/eslint.js --max-warnings=0 src/",
    "lint-fix": "node_modules/eslint/bin/eslint.js src/ --fix",
    "node": "node_modules/.bin/babel-node"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "cz-conventional-changelog": "^3.1.0",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "fetch-mock": "^7.7.0",
    "history": "^4.10.1",
    "node-fetch": "^2.6.0",
    "react-scripts": "^3.4.0",
    "typescript": "^3.8.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
