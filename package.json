{
  "name": "renga-ui",
  "version": "0.1.0-SNAPSHOT",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run sass && npm run copy:material && concurrently --kill-others \"webpack --progress --colors\"",
    "dev": "cross-env NODE_ENV=development npm run sass && npm run copy:material && concurrently --kill-others \"webpack --progress --colors --watch\" \"node-sass --watch --include-path node_modules --output ./src/css ./src/sass/main.scss\" \"npm run backend\"",
    "copy:material": "ncp node_modules/material-components-web/dist/material-components-web.css src/css/material-components-web.css && ncp node_modules/material-components-web/dist/material-components-web.js dist/js/material-components-web.js",
    "sass": "node-sass --include-path node_modules --output ./src/css ./src/sass/main.scss",
    "backend": "FLASK_DEBUG=1 APPLICATION_ROOT=/ui python server/run.py",
    "lint": "tslint src/**/*.ts"
  },
  "dependencies": {
    "cytoscape": "^3.1.4",
    "cytoscape-qtip": "^2.7.1",
    "downloadjs": "^1.4.7",
    "fetch-readablestream": "^0.1.0",
    "lodash": "^4.17.4",
    "material-components-web": "^0.16.0",
    "text-encoding": "^0.6.4",
    "typescript": "^2.4.2",
    "vue": "^2.3.4",
    "vue-class-component": "^5.0.2",
    "vue-lodash": "^1.0.3",
    "vue-property-decorator": "^5.1.0",
    "vuetify": "^0.14.11",
    "web-streams-polyfill": "^1.3.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@types/node": "^8.0.7",
    "awesome-typescript-loader": "^3.2.1",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "ncp": "^2.0.0",
    "node-sass": "^4.5.3",
    "raw-loader": "^0.5.1",
    "tslint": "^5.4.3",
    "tslint-loader": "^3.5.3",
    "vue-router": "^2.7.0",
    "webpack": "^3.0.0"
  }
}
