{
  "name": "renga-ui",
  "version": "0.1.0-SNAPSHOT",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run clean && npm run compile",
    "clean": "rimraf dist",
    "compile": "webpack --config config/webpack.config.prod.js",
    "dev": "cross-env NODE_ENV=development concurrently --kill-others \"npm run dev:compile\" \"npm run dev:backend\"",
    "dev:backend": "FLASK_DEBUG=1 APPLICATION_ROOT=/ui FLASK_APP=server/app/__init__.py flask run",
    "dev:compile": "webpack --progress --colors --watch --config config/webpack.config.dev.js",
    "lint": "tslint src/**/*.ts"
  },
  "dependencies": {
    "d3": "^4.11.0",
    "fetch-readablestream": "^0.1.0",
    "raven-js": "^3.20.1",
    "text-encoding": "^0.6.4",
    "vue": "~2.4.4",
    "vue-class-component": "^5.0.2",
    "vue-full-screen-file-drop": "^1.0.0",
    "vue-property-decorator": "^5.3.0",
    "vue-router": "^2.7.0",
    "vuetify": "^0.15.7",
    "web-streams-polyfill": "^1.3.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@types/d3": "^4.11.0",
    "@types/node": "^8.0.28",
    "awesome-typescript-loader": "^3.2.3",
    "babel-minify-webpack-plugin": "^0.2.0",
    "compression-webpack-plugin": "^1.0.0",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.2",
    "glob": "^7.1.2",
    "ncp": "^2.0.0",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "tslint": "^5.7.0",
    "tslint-loader": "^3.5.3",
    "typescript": "~2.5.2",
    "webpack": "~3.5.6",
    "webpack-manifest-plugin": "^1.3.2"
  }
}
